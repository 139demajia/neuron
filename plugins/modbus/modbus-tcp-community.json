{
	"tag_regex": [
		{
			"type": 3,
			"regex": "^[0-9]+![3-4][0-9]+(#B|#L|)$"
		},
		{
			"type": 4,
			"regex": "^[0-9]+![3-4][0-9]+(#B|#L|)$"
		},
		{
			"type": 5,
			"regex": "^[0-9]+![3-4][0-9]+(#BB|#BL|#LL|#LB|)$"
		},
		{
			"type": 6,
			"regex": "^[0-9]+![3-4][0-9]+(#BB|#BL|#LL|#LB|)$"
		},
		{
			"type": 9,
			"regex": "^[0-9]+![3-4][0-9]+(#BB|#BL|#LL|#LB|)$"
		},
		{
			"type": 11,
			"regex": "^[0-9]+!([0-1][0-9]+|[3-4][0-9]+\\.([0-9]|[0-1][0-5]))$"
		}
	],
	"group_interval": 1000,
	"host": {
		"name": "Device IP Address",
		"name_zh": "设备IP地址",
		"description": "Target device IPv4 address",
		"description_zh": "目标设备IPv4地址",
		"attribute": "required",
		"type": "string",
		"valid": {
			"regex": "/^((2[0-4]\\d|25[0-5]|[01]?\\d\\d?)\\.){3}(2[0-4]\\d|25[0-5]|[01]?\\d\\d?)$/",
			"length": 30
		}
	},
	"port": {
		"name": "Device Port",
		"name_zh": "设备端口",
		"description": "Target device port number",
		"description_zh": "目标设备端口号",
		"attribute": "required",
		"type": "int",
		"default": 502,
		"valid": {
			"min": 1,
			"max": 65535
		}
	},
	"timeout": {
		"name": "Recv Timeout",
		"name_zh": "接收超时时间",
		"description": "Recv msg timeout(ms)",
		"description_zh": "接收消息超时时间(毫秒)",
		"attribute": "required",
		"type": "int",
		"default": 3000,
		"valid": {
			"min": 1000,
			"max": 65535
		}
	}
}